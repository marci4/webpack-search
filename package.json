{
  "name": "license-n-package-collector",
  "author": {
    "name": "Marcel Prestel",
    "email": "admin@marci4.de"
  },
  "version": "0.1.3",
  "license": "Beerware",
  "dependencies": {
    "yargs": "^13.2.4",
    "got": "9.4.0"
  },
  "engines": {
    "node": ">=0.12"
  },
  "files": [
    "index.js"
  ],
  "main": "./index.js",
  "bin": {
    "license-n-package-collector": "./index.js"
  },
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -cw",
    "test": "jest",
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint"
  },
  "devDependencies": {
    "@types/node": "11.13.12",
    "@types/yargs": "13.0.0",
    "@types/got": "^9.4.4",
    "jest": "24.8.0",
    "typescript": "3.5.1",
    "ts-jest": "24.0.2",
    "@types/jest": "24.0.11",
    "@types/nock": "^10.0.3",
    "tslint": "5.16.0",
    "rollup-plugin-typescript2": "^0.21.1",
    "rollup": "^1.12.5",
    "nock": "11.0.0-beta.17"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/src/**",
      "!src/index.ts"
    ]
  }
}
